{
    "version": 2,
    "builds": [
        {
            "src": "frontend/**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      },
      {
        "src": "^/(ads\\.txt|sitemap\\.xml)$",
        "dest": "/public/$1"
      }
    ]
  }